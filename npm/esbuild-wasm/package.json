{
  "name": "esbuild-wasm",
  "version": "0.6.27",
  "description": "The cross-platform WebAssembly binary for esbuild, a JavaScript bundler.",
  "repository": "https://github.com/evanw/esbuild",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "lib/main.js",
  "browser": "lib/browser.js",
  "module": "lib/browser.mjs",
  "exports": {
    ".": {
      "browser": {
        "module": "./lib/browser.mjs",
        "script": "./lib/browser.js"
      },
      "node": "./lib/main.js"
    },
    "./": "./"
  },
  "types": "lib/main.d.ts",
  "directories": {
    "bin": "bin"
  }
}
